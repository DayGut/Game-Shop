<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="css/agregar_editar.css">
    <link href="https://fonts.googleapis.com/css2?family=Roboto:wght@100&display=swap">
    <script defer src="js/script.js"></script>
    <title>Game Shop</title>
</head>

<body>
    <!-- header-->
    <header class="topheader">
        <nav class="topnav">
            <a href="/" class="logo">
                <img src="/images/logo.jpg" alt="logo Game Shop">
            </a>
            <button class="open-menu">
                <a class="fa-solid fa-bars"></a>

            </button>
            <ul class="menu">
                <button class="close-menu">
                    <a class="fa-solid fa-xmark" aria-label="cerrar menu"></a>
                </button>
                <li><a href="#">buscador</a></li>
                <li><a href="/home">home</a></li>
                <li><a href="/productDetail">shop</a></li>
                <li><a href="/register">register</a></li>
                <li><a href="/login">login</a></li>
                <li><a href="/productCart">carrito</a></li>
            </ul>
        </nav>
    </header>
    <!--fin header-->
    <!-- main-->
    <div class="form_container">
        <form action="form" method="POST">
            <ul>
             <li>
               <label for="nombre_producto">producto: <%=  %> </label>
               <input type="text" id="nombre_producto" name="producto" >
             </li>
             <li>
               <label for="text_detalle">detalle:</label>
               <textarea type="email" id="text_detalle" name="detalle"></textarea>
             </li>
             <li>
               <label for="precio">precio:</label>
               <input id="precio" name="precio">
             </li>
             <li>
               <label for="img_foto">foto:</label>
               <input id="img_foto" name="foto">
             </li>
            </ul>
           </form>
    </div>
    <!-- fin main -->

    <!-- footer-->
    <footer class="pie-pagina">
        <div class="grupo-1">
            <div class="logo">
                <figure>
                    <a href="/" alt="logo">
                        <img src="images/logo.jpg" alt="">
                    </a>
                </figure>
            </div>
            <div class="titulo-nostros">
                <h2>SOBRE NOSOTROS</h2>
                <p>Lorem ipsum dolor sit amet consectetur adipisicing.</p>

            </div>
            <div class="titulo-redes">
                <h2>SEGUINOS</h2>

                <div class="redes">
                    <div class="icon facebook">
                        <div class="tooltip">Facebook</div>
                        <span><i class="fab fa-facebook-f"></i></span>
                    </div>
                    <div class="icon twitter">
                        <div class="tooltip">Twitter</div>
                        <span><i class="fab fa-twitter"></i></span>
                    </div>
                    <div class="icon instagram">
                        <div class="tooltip">Instagram</div>
                        <span><i class="fab fa-instagram"></i></span>
                    </div>
                    <div class="icon github">
                        <div class="tooltip">Github</div>
                        <span><i class="fab fa-github"></i></span>
                    </div>
                    <div class="icon youtube">
                        <div class="tooltip">Youtube</div>
                        <span><i class="fab fa-youtube"></i></span>
                    </div>
                </div>
            </div>
        </div>
        <div class="Copyright">
            <small>&copy; 2022- Todos los Derechos reservados</small>
        </div>
    </footer>
    <!-- fin footer-->



    <script src="https://kit.fontawesome.com/f21216c2d3.js" crossorigin="anonymous"></script>
</body>

</html>